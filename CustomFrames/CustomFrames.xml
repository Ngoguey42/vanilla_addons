		<Frame name="customXPvalsframe" parent="MainMenuExpBar" enableMouse="false" frameStrata="HIGH">
			<Backdrop bgFile="Interface\ChatFrame\ChatFrameBackground" tile="false"/>
			<Size x="400" y="20" />
			<Anchors>
				<Anchor point="CENTER" relativeTo="$parent" RelativePoint="CENTER" />
			</Anchors>
			<Layers>
				<Layer frameStrata="DIALOG">
						<FontString name="$parentText" inherits="GameFontNormalLarge" maxLines = "1" SetAllPoints="true"/>
				</Layer>
			</Layers>
			<Scripts>
				<OnLoad>
					function customXPvalsframe:UpdateText(CurXP)
						if CurXP == nil then
							print("nil curxp");
							return ;
						end
						local MaxXP = UnitXPMax("player");
						local CurPerc = CurXP * 100 / MaxXP;
						local CurDeficit = MaxXP - CurXP;
						local CurPercDeficit = floor((100 - CurPerc) * 10) / 10;
						CurPerc = floor((CurPerc) * 10) / 10;
						
						customXPvalsframeText:SetText("("..CurPerc.."%) "..CurXP.."/"..MaxXP.." (-"..CurDeficit..")(-"..CurPercDeficit.."%)");

						
					end
				</OnLoad>
			</Scripts>
		</Frame>
		
		
		<Frame name="customMoneyframe" parent="MainMenuBarBackpackButton" enableMouse="false" frameStrata="HIGH">
