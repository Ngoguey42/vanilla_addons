				</Layer>
			</Layers>
			<Scripts>
				<OnLoad>
					function customMoneyframe:UpdateText()
						local CurCopper = GetMoney("player");
						
						local Copper = mod(CurCopper, 100);
						local Silver = mod(CurCopper / 100, 100);
						local Gold = CurCopper / 10000 ;
						
						Copper = floor(Copper);
						Silver = floor(Silver);
						Gold = floor(Gold);
						customMoneyframeText:SetText(Gold..":"..Silver..":"..Copper);
					end

				</OnLoad>
			</Scripts>
		</Frame>
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
