<Ui xmlns="http://www.blizzard.com/wow/ui/"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

<Script file="ngoplayer.lua"/>

<!-- EnvFrames -->
<Frame name="ngoplayer" frameStrata="high" enableMouse="true" movable="true" parent="UIParent">
	<!-- Scripts -->
	<Scripts>
		<OnLoad>
		ngoplayer_onload();
		</OnLoad>
		<OnEvent>ngoplayer_onevent(event, arg1, arg2);</OnEvent>
	</Scripts>
	<!-- Backdrops -->
	<Backdrop bgFile="Interface\ChatFrame\ChatFrameBackground" tile="false">
	</Backdrop>
	<!-- Frames -->
	<Frames>
		<!-- hp absolute -->
		<Frame name="$parenthpa" enableMouse="false" frameStrata="HIGH">
			<Scripts> <OnLoad>
				ngoplayer_setDoubleParentKey(this, this:GetParent())
				this:SetParent(PlayerFrame);
				this:SetFrameStrata("HIGH");
			</OnLoad> </Scripts>
			<Size x="140" y="20" />
			<Layers>
				<Layer frameStrata="DIALOG">
					<FontString name="$parentt" inherits="GameFontNormalLarge" maxLines = "1" SetAllPoints="true"/>
				</Layer>
			</Layers>
		</Frame>

